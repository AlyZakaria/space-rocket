{
    "run.timelimit": 300000,
    
    "run.compiler": "mingw32-g++.exe -std=c++11 -I./include -Wall -g -IC:MinGWinclude ",
    "run.compilerArgs": "-o Program.exe -L C:MinGWlib -lglut32 -lopengl32 -lglu32 -lwinmm -lgdi32",
    "C_Cpp.formatting": "vcFormat",
    "breadcrumbs.enabled": false,
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "files.exclude": {
      "mingw*": true,
      "Program.exe": true
    },
    "files.associations": {
      "iostream": "cpp",
      "fstream": "cpp",
      "ostream": "cpp",
      "array": "cpp",
      "deque": "cpp",
      "forward_list": "cpp",
      "list": "cpp",
      "string": "cpp",
      "unordered_map": "cpp",
      "unordered_set": "cpp",
      "vector": "cpp",
      "initializer_list": "cpp",
      "regex": "cpp",
      "bitset": "cpp",
      "iosfwd": "cpp",
      "new": "cpp",
      "mouse.cpp": "c",
      "cmath": "cpp",
      "atomic": "cpp",
      "*.tcc": "cpp",
      "cctype": "cpp",
      "cfenv": "cpp",
      "chrono": "cpp",
      "cinttypes": "cpp",
      "clocale": "cpp",
      "complex": "cpp",
      "condition_variable": "cpp",
      "csetjmp": "cpp",
      "csignal": "cpp",
      "cstdarg": "cpp",
      "cstddef": "cpp",
      "cstdint": "cpp",
      "cstdio": "cpp",
      "cstdlib": "cpp",
      "cstring": "cpp",
      "ctime": "cpp",
      "cwchar": "cpp",
      "cwctype": "cpp",
      "exception": "cpp",
      "functional": "cpp",
      "future": "cpp",
      "iomanip": "cpp",
      "istream": "cpp",
      "limits": "cpp",
      "mutex": "cpp",
      "numeric": "cpp",
      "ratio": "cpp",
      "scoped_allocator": "cpp",
      "sstream": "cpp",
      "stdexcept": "cpp",
      "streambuf": "cpp",
      "system_error": "cpp",
      "thread": "cpp",
      "tuple": "cpp",
      "type_traits": "cpp",
      "utility": "cpp",
      "typeindex": "cpp",
      "typeinfo": "cpp",
      "valarray": "cpp"
    },
    "code-runner.executorMap":{
      "cpp": "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
  },
  "C_Cpp.errorSquiggles": "enabled",
  } 